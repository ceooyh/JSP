	<!-- 선언문 -->
	<%
		int num;
		int result = 0;
		
		for(num=2;num<10;num++){
			for(int i=1;i<10;i++){
				result = num * i;
				response.getWriter().println(num+"단 = "+num + "*" + i + " = " + result+"<br>");
			}
		}
	%>
	
