	<h2>구구단 출력 결과물</h2>
	
	<%
		int num;
		int result = 0;
		
		for(num=2;num<10;num++){
			for(int i=1;i<10;i++){
				result = num * i;
				response.getWriter().println(num + "*" + i + " = " + result+"<br>");
			}
				response.getWriter().println("-------------------------------------<br>");
		}
	%>
	<h1>구구단 출력 2번째</h1>
	<%!
		public String getGuGu(int n){
			String result = "";
			for(n=2;n<10;n++){
				for(int i=1;i<10;i++){
					result += n + "*" + i + " = " + result+"<br>";
				}
				return result;
			}
		}
	%>
	
