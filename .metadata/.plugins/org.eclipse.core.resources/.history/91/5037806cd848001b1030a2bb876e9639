<body>
	<!-- 선언문 -->
	<%
		int num;
		int result = 0;
		
		public int gugudan(){
			for(num=2;num<10;num++){
				for(int i=1;i<10;i++){
					result = num * i;
					response.getWriter().println(num + "*");
				}
			}
			return result;
		}
	%>
	<%=gugudan()	%>
	
	<%
		response.getWriter().println(result);
	%>
</body>
